{
    "name": "magentron/laravel-blade-lint",
    "description": "Laravel Blade Lint console command",
    "type": "library",
	"keywords": ["laravel","blade","lint","console"],
	"homepage": "https://github.com/Magentron/laravel-blade-lint",
	"time": "2018-11-28 14:07:44",
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Jeroen Derks",
            "email": "jeroen@derks.it",
			"homepage": "https://www.derks.it/",
			"role": "Developer"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
		"ext-pcntl": "*",
		"laravel/framework": "*"
	},
    "require-dev": {
        "barryvdh/laravel-ide-helper": "*",
        "friendsofphp/php-cs-fixer": "*",
        "phpmd/phpmd": "*",
        "phpstan/phpstan": "*",
        "sebastian/phpcpd": "*",
        "vimeo/psalm": "*"
    },
	"autoload": {
		"psr-4": {
			"Magentron\\BladeLint\\": "src/Magentron/BladeLint"
		}
	},
	"extra": {
		"laravel": {
			"providers": [
				"Magentron\\BladeLint\\Providers\\ServiceProvider"
			]
		}
	},
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "treeware/plant": true,
            "php-http/discovery": true
        }
    }
}
